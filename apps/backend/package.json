    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "source-map-support": "^0.5.21",
    "uuid": "^13.0.0",
    "validator": "^13.15.15"
